{
  "name": "vue3-elelment-plus-admin",
  "private": true,
  "version": "0.0.1",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint --ext packages/**/*.{js,ts,vue}",
